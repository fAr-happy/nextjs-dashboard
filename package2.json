    "lint:circle": "ENABLE_CIRCULAR_CHECK=true next lint .",
    "lint:fix": "next lint . --fix",
    "lint:report": "ENABLE_CIRCULAR_CHECK=true REPORT_FILE=scripts/validate-deps/eslint-errors-$(date '+%Y.%m.%d-%H.%M.%S').html && yarn lint --output-file $REPORT_FILE --format html && open $REPORT_FILE",

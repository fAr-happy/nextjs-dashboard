    "build-storybook": "yarn storybook:pre && storybook build",
    "test:pre": "yarn scripts:minify-ts && yarn scripts:test-preparation",
    "test": "yarn test:pre && jest --silent",
    "test:watch": "yarn test:pre && jest --watch",
    "test:eslint-config": "yarn eslint .eslint-test --report-unused-disable-directives",
    "build:analyze": "ANALYZE=true yarn build",

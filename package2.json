    "build:post": "yarn scripts:delete-sourcemaps && yarn scripts:prepare-standalone && yarn scripts:compressor-ssg --compress && yarn scripts:build-single END",
    "build": "yarn build:pre && next build && yarn build:post",
    "build:local": "ZIP=false && yarn scripts:prepare-env staging && yarn build",
    "build:staging": "yarn scripts:prepare-env staging && yarn build",
    "build:production": "yarn scripts:prepare-env production && yarn build",
    "start:pre": "yarn scripts:compressor-ssg --extract",
    "start": "yarn start:pre && env-cmd -f .env --use-shell \"node run-boost.js\"",
